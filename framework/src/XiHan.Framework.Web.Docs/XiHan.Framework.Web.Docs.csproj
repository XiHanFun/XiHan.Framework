<Project Sdk="Microsoft.NET.Sdk.Web">

	<Import Project="..\..\props\netcore.props" />
	<Import Project="..\..\props\common.props" />
	<Import Project="..\..\props\version.props" />
	<Import Project="..\..\props\nuget.props" />

	<PropertyGroup>
		<Title>XiHan.Framework.Web.Docs</Title>
		<AssemblyName>XiHan.Framework.Web.Docs</AssemblyName>
		<PackageId>XiHan.Framework.Web.Docs</PackageId>
		<Description>曦寒框架 Web 核心文档库</Description>
		<OutputType>Library</OutputType>
		<NoDefaultLaunchSettingsFile>true</NoDefaultLaunchSettingsFile>
		<GenerateDocumentationFile>false</GenerateDocumentationFile>
		<OpenApiGenerateDocuments>false</OpenApiGenerateDocuments>
		<OpenApiGenerateDocumentsOnBuild>false</OpenApiGenerateDocumentsOnBuild>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="index.html" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="index.html" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Scalar.AspNetCore" Version="2.8.5" />
		<PackageReference Include="Swashbuckle.AspNetCore" Version="9.0.4" />
		<PackageReference Include="Swashbuckle.AspNetCore.Filters" Version="9.0.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\XiHan.Framework.Web.Api\XiHan.Framework.Web.Api.csproj" />
	</ItemGroup>

</Project>
